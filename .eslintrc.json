{
    "env": {
      "browser": true,
      "node": true,
      "es2021": true
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended", "eslint-config-prettier"],
    "parserOptions": {
      "ecmaFeatures": {

      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "semi": [2, "always"],
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    }
  }
  